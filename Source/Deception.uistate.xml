<?xml version="1.0" encoding="utf-8" ?>
<c2uistate>
    <!--This is a UI state file - its content describes the user interface settings when the project was last open.
It is entirely optional and the project will load without it.  If you are placing the project under source control,
you probably do not want to add any .uistate.xml files to the repository.-->
    <configurations>
        <active>
            <configuration name="HTML5" />
        </active>
        <display name="HTML5" />
    </configurations>
    <open-tabs>
        <layout order="10">TItle Screen</layout>
        <layout active="1" order="12">Game</layout>
        <event-sheet order="0">Game Events</event-sheet>
        <event-sheet order="1">Initialisation and Scoring</event-sheet>
        <event-sheet order="2">Globals</event-sheet>
        <event-sheet order="3">Text Info Management</event-sheet>
        <event-sheet order="4">Buttons and Launching</event-sheet>
        <event-sheet order="5">Planet and Guns</event-sheet>
        <event-sheet order="6">Twinkling Stars</event-sheet>
        <event-sheet order="7">Rockets and Collisions</event-sheet>
        <event-sheet order="8">Transitions</event-sheet>
        <event-sheet order="9">Introduction</event-sheet>
        <event-sheet order="11">Title Screen</event-sheet>
    </open-tabs>
    <properties>
        <property expanded="1" name="About" />
        <property expanded="1" name="Project settings" />
        <property expanded="0" name="Window Size" />
        <property expanded="1" name="Configuration Settings" />
    </properties>
    <families-expanded root="1">
        <family expanded="1" name="Colourable" />
    </families-expanded>
    <folders-expanded>
        <folder expanded="1" path="object-folder/" />
        <folder expanded="0" path="object-folder/Text Objects/" />
        <folder expanded="0" path="object-folder/Utility Objects/" />
        <folder expanded="1" path="layout-folder/" />
        <folder expanded="1" path="event-folder/" />
        <folder expanded="1" path="file-folder/" />
        <folder expanded="0" path="sound-folder/" />
        <folder expanded="1" path="music-folder/" />
        <folder expanded="0" path="file-folder/Icons/" />
        <folder expanded="1" path="animation-folder/ButtonBackground/" />
        <folder expanded="1" path="animation-folder/MoonLifeMarker/" />
        <folder expanded="1" path="animation-folder/Star/" />
        <folder expanded="1" path="animation-folder/Button/" />
        <folder expanded="1" path="animation-folder/Moon/" />
        <folder expanded="1" path="animation-folder/Launcher/" />
        <folder expanded="1" path="animation-folder/Missile/" />
        <folder expanded="1" path="animation-folder/Rocket/" />
    </folders-expanded>
</c2uistate>
